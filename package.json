    "tmp": "0.0.31",
    "ts-node": "^2.1.2"
  },
  "devDependencies": {
    "@types/chai": "^4.0.0",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.13",
    "@types/sinon": "^2.3.1",
    "@types/sinon-chai": "^2.7.28",
    "chai": "^4.0.2",
    "codecov": "^2.2.0",
    "mocha": "^3.4.2",
    "npm-prepublish": "^1.2.2",
    "nyc": "^11.0.2",
    "sinon": "^2.3.4",
    "sinon-chai": "^2.11.0",
    "tslint": "^5.4.3",
    "typescript": "^2.3.4"
  },
  "nyc": {
    "extension": [
      ".ts"
