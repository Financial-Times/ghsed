    "@types/chalk": "^0.4.31",
    "@types/diff": "^0.0.31",
    "@types/git": "0.0.18",
    "@types/inquirer": "^0.0.44",
    "@types/lodash": "^4.14.63",
    "@types/meow": "^3.6.2",
    "@types/shelljs": "^0.7.2",
    "@types/tmp": "0.0.33",
    "chalk": "^1.1.3",
    "diff": "^3.2.0",
    "github-api": "^3.0.0",
    "inquirer": "^3.0.6",
    "lodash": "^4.17.4",
    "meow": "^3.7.0",
    "node-git": "0.0.0",
    "parse-sed": "github:aendrew/parse-sed#0.1.0",
